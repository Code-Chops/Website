<div class="nav-item">
    <a class="nav-link pointerCursor noSelect" href="@(Data.HRef ?? NavigationManager.Uri.Split('#').First())" @onclick="@(e => Data.OnClick(e, Data))">
        <span class="@If(Data.FontAwesomeIcon is not null, $"fa {Data.FontAwesomeIcon}")"></span> @Data.Text @ChildContent
    </a>
</div>
